{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AMCode": {
    "OCR": {
      "DefaultProvider": "Azure",
      "FallbackProviders": ["AWS", "Google"],
      "ConfidenceThreshold": 0.7,
      "MaxRetries": 3,
      "EnableFallbackProviders": true,
      "MaxFallbackProviders": 2,
      "EnableBatchProcessing": true,
      "MaxBatchSize": 10,
      "Azure": {
        "SubscriptionKey": "your-azure-subscription-key",
        "Endpoint": "https://your-endpoint.cognitiveservices.azure.com/",
        "Region": "eastus",
        "CostPerRequest": 0.001,
        "AverageResponseTime": "00:00:03",
        "MaxImageSizeMB": 20,
        "SupportedLanguages": ["en", "es", "fr", "de", "it", "pt", "nl", "pl", "ru", "zh", "ja", "ko"]
      },
      "AWS": {
        "Region": "us-east-1",
        "AccessKey": "your-aws-access-key",
        "SecretKey": "your-aws-secret-key",
        "CostPerRequest": 0.0015,
        "AverageResponseTime": "00:00:04",
        "MaxImageSizeMB": 10,
        "SupportedLanguages": ["en", "es", "fr", "de", "it", "pt", "nl", "pl", "ru", "zh", "ja", "ko"]
      },
      "Google": {
        "ProjectId": "your-google-project-id",
        "CredentialsPath": "path/to/your/credentials.json",
        "CostPerRequest": 0.0008,
        "AverageResponseTime": "00:00:02",
        "MaxImageSizeMB": 20,
        "SupportedLanguages": ["en", "es", "fr", "de", "it", "pt", "nl", "pl", "ru", "zh", "ja", "ko"]
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  }
}
